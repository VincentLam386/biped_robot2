          izz="${mass_abad*(height_abad*height_abad+width_abad*width_abad)/12}" />
    </inertial>
  </link>

  <joint name="right_front_joint" type="continuous">
    <parent link="leg_right_abad"/>
    <child link="leg_right_front_top"/>
    <origin xyz="0 ${-width_hip/2-height_abad-space} 0" rpy="0 3.1416 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.7"/>
  </joint>


  <joint name="right_rear_joint" type="continuous">
    <parent link="leg_right_abad"/>
    <child link="leg_right_rear_top"/>
    <!--origin xyz="0 ${-width_hip/2-height_abad-width_leg/2-space} 0" rpy="0 -2.618 0"/-->
    <origin xyz="0 ${-width_hip/2-height_abad-width_leg-space*2} 0" rpy="0 3.1416 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.7"/>
  </joint>

  <!-- Right front top leg link -->
  <link name="leg_right_front_top">
    <collision>
      <origin xyz="0 ${-width_leg/2-space} ${height_front_top/2-width_leg}" rpy="0 0 0"/>
      <geometry>
        <box size="${length_leg} ${width_leg} ${height_front_top}"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 ${-width_leg/2-space} ${height_front_top/2-width_leg}" rpy="0 0 0"/>
      <geometry>
        <box size="${length_leg} ${width_leg} ${height_front_top}"/>
      </geometry>
      <material name="black"/>
    </visual>

    <inertial>
      <origin xyz="0 ${-width_leg/2-space} ${height_front_top/2-width_leg}" rpy="0 0 0"/>
      <mass value="${mass_leg}"/>
      <inertia
        ixx="${mass_leg / 12.0 * (width_leg*width_leg + height_front_top*height_front_top)}" ixy="0.0" ixz="0.0"
          iyy="${mass_leg / 12.0 * (height_front_top*height_rear_top + width_leg*width_leg)}" iyz="0.0"
          izz="${mass_leg / 12.0 * (width_leg*width_leg + width_leg*width_leg)}" />
    </inertial>
  </link>


  <!-- right rear top leg link -->
  <link name="leg_right_rear_top">
    <collision>
      <origin xyz="0 ${-width_leg/2-space} ${height_rear_top/2-width_leg}" rpy="0 0 0"/>
      <geometry>
        <box size="${length_leg} ${width_leg} ${height_rear_top}"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 ${-width_leg/2-space} ${height_rear_top/2-width_leg}" rpy="0 0 0"/>
      <geometry>
        <box size="${length_leg} ${width_leg} ${height_rear_top}"/>
      </geometry>
      <material name="black"/>
    </visual>

    <inertial>
      <origin xyz="0 ${-width_leg/2-space} ${height_rear_top/2-width_leg}" rpy="0 0 0"/>
      <mass value="${mass_leg}"/>
      <inertia
        ixx="${mass_leg / 12.0 * (width_leg*width_leg + height_rear_top*height_rear_top)}" ixy="0.0" ixz="0.0"
          iyy="${mass_leg / 12.0 * (height_rear_top*height_rear_top + width_leg*width_leg)}" iyz="0.0"
          izz="${mass_leg / 12.0 * (width_leg*width_leg + width_leg*width_leg)}" />
    </inertial>
  </link>

  <joint name="right_front_joint2" type="continuous">
    <parent link="leg_right_front_top"/>
    <child link="leg_right_front_bot"/>
    <origin xyz="0 ${-width_leg-space} ${height_front_top-width_leg}" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.7"/>
  </joint>

  <!-- right front bottom leg link -->
  <link name="leg_right_front_bot">
    <collision>
      <origin xyz="0 ${-width_leg/2-space} ${height_front_bot/2-width_leg}" rpy="0 0 0"/>
      <geometry>
        <box size="${length_leg} ${width_leg} ${height_front_bot}"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 ${-width_leg/2-space} ${height_front_bot/2-width_leg}" rpy="0 0 0"/>
      <geometry>
        <box size="${length_leg} ${width_leg} ${height_front_bot}"/>
      </geometry>
      <material name="black"/>
    </visual>
    <inertial>
      <origin xyz="0 ${-width_leg/2-space} ${height_front_bot/2-width_leg}" rpy="0 0 0"/>
      <mass value="${mass_leg}"/>
      <inertia
        ixx="${mass_leg / 12.0 * (width_leg*width_leg + height_front_bot*height_front_bot)}" ixy="0.0" ixz="0.0"
          iyy="${mass_leg / 12.0 * (height_front_bot*height_front_bot + width_leg*width_leg)}" iyz="0.0"
          izz="${mass_leg / 12.0 * (width_leg*width_leg + width_leg*width_leg)}" />
    </inertial>
  </link>


  <joint name="right_rear_joint2" type="continuous">
    <parent link="leg_right_rear_top"/>
    <child link="leg_right_rear_bot"/>
    <!--origin xyz="0 ${space} ${height_rear_top-width_leg}" rpy="0 -1.0472 0"/-->
    <origin xyz="0 ${space} ${height_rear_top-width_leg}" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <dynamics damping="0.7"/>
  </joint>

  <!-- right rear bottom leg link -->
  <link name="leg_right_rear_bot">
    <collision>
      <origin xyz="0 ${width_leg/2-space} ${height_rear_bot/2-width_leg}" rpy="0 0 0"/>
      <geometry>
        <box size="${length_leg} ${width_leg} ${height_rear_bot}"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 ${width_leg/2-space} ${height_rear_bot/2-width_leg}" rpy="0 0 0"/>
      <geometry>
        <box size="${length_leg} ${width_leg} ${height_rear_bot}"/>
      </geometry>
      <material name="black"/>
    </visual>

    <inertial>
      <origin xyz="0 ${width_leg/2-space} ${height_rear_bot/2-width_leg}" rpy="0 0 0"/>
      <mass value="${mass_leg}"/>
      <inertia
        ixx="${mass_leg / 12.0 * (width_leg*width_leg + height_rear_bot*height_rear_bot)}" ixy="0.0" ixz="0.0"
          iyy="${mass_leg / 12.0 * (height_rear_bot*height_rear_bot + width_leg*width_leg)}" iyz="0.0"
          izz="${mass_leg / 12.0 * (width_leg*width_leg + width_leg*width_leg)}" />
    </inertial>
  </link>

  <joint name="right_end_joint" type="fixed">
    <origin xyz="0 ${width_leg/2-space} ${height_rear_bot-width_leg}" rpy="0 0 0" />
    <parent link="leg_right_rear_bot" />
    <child link="right_end_tip" />
  </joint>

  <link name="right_end_tip">
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <sphere radius="${radius_tip}"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
	      <sphere radius="${radius_tip}"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="${mass_tip}"/>
      <inertia
        ixx="0.0" ixy="0.0" ixz="0.0"
          iyy="0.0" iyz="0.0"
          izz="0.0" />
    </inertial>
  </link>

  <!-- Define transmission -->
  <!-- Left -->
  <transmission name="left_abad">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="left_abad_joint">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="left_abad_motor">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
    </actuator>
  </transmission>

  <transmission name="left_rear2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="left_rear_joint2">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="left_rear_motor2">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
